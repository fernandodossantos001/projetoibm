package br.com.deschateie.bo;

import br.com.deschateie.beans.FormaPagamento;
import br.com.deschateie.dao.FormaPagamentoDAO;

public class FormaPagamentoBO {
	public static FormaPagamento pesquisarFormaPagamento(FormaPagamento fb)throws Exception{
		if(fb.getCodFormaPagamento()<0) {
			return new FormaPagamento();
		}
		
		if (fb.getCodFormaPagamento()>99) {
			return new FormaPagamento();
		}
		
		FormaPagamentoDAO dao = new FormaPagamentoDAO().consultarFormaPagamento(fp);
	}

}
