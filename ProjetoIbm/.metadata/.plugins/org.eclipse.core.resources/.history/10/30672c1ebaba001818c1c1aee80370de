public class TesteAgendamento {

	public static void main(String[] args) {

		try {
			System.out.println(new AgendamentoDAO().consultarAgendamento(103));
		} catch (Exception e) {
			e.printStackTrace();
			System.out.println(Excecao.tratarExcecao(e));
		}
	}

}
