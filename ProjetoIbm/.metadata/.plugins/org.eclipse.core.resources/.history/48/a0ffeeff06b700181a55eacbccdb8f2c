package br.com.deschateie.teste;

import br.com.deschateie.dao.EnderecoDAO;
import br.com.deschateie.excecao.Excecao;

public class TesteAtualizarEndereco {

	public static void main(String[] args) {

		try {
			System.out.println(new EnderecoDAO().atualizar("Jaguaraçu", "Rua", "53", "Casa",
					"03930080", "Jardim São Cristóvão", "São Paulo", "SP", "Brasil"));
		} catch (Exception e) {
			e.printStackTrace();
			System.out.println(Excecao.tratarExcecao(e));
		}
	}

}
