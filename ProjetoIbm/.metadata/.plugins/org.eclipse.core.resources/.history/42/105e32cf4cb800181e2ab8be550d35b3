package br.com.deschateie.bo;


public class PacienteBO {
	
	public static String consutlarPaciente(int codPaciente)throws Exception{
		
		if(codPaciente <0) {
			return "código inválido";
		}
		if(codPaciente> 5) {
			return "código muito grande";
		}
		
		UsuarioDAO() dao = new UsuarioDAO();
		
	}

}
